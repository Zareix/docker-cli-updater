{
  "name": "docker-swarm-updater",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "start": "bun run src/index.ts",
    "update-all": "bun run src/index.ts update",
    "check": "bunx @biomejs/biome check --apply src/",
    "build": "bun build --target=bun --outfile=./dist/index.js src/index.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@types/dockerode": "^3.3.23",
    "bun-types": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@inquirer/prompts": "^3.3.0",
    "chalk": "^5.3.0",
    "chalk-template": "^1.1.0",
    "commander": "^11.1.0",
    "dockerode": "^4.0.2",
    "ora": "^8.0.1"
  }
}